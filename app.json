{
  "expo": {
    "name": "shareAndcare",
    "slug": "shareAndcare",
    "version": "2.0.1",
    "sdkVersion": "39.0.0",
    "orientation": "portrait",
    "icon": "./images/icon.png",
    "splash": {
      "image": "./assets/acslogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shareandcare.shareAndcare"
    },
    "android": {
      "package": "com.shareandcare.shareAndcare"
    },
    "web": {
      "build": {
        "babel": {
          "include": ["static-container"]
        }
      },
      "favicon": "./images/icon.png"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}
